{
  "name": "@capraconsulting/validation-utils-js",
  "version": "0.0.0-development",
  "description": "Utils for valdiation of input fields",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "build": "rimraf es dist && rollup -c",
