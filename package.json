  "devDependencies": {
    "@types/jest": "24.0.11",
    "@types/jest-each": "0.3.4",
    "jest": "24.7.1",
    "jest-each": "24.7.1",
    "microbundle": "0.11.0",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "tslint": "5.15.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.2"
  }
