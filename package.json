    "@types/jest": "29.5.9",
    "@types/jest-each": "0.3.4",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-each": "29.7.0",
    "prettier": "3.1.0",
    "rimraf": "5.0.5",
    "rollup": "3.29.4",
    "rollup-plugin-typescript2": "0.36.0",
    "semantic-release": "22.0.8",
    "ts-jest": "29.1.1",
    "tslint": "5.20.1",
